  },
  "cpp": {
    "tokenizer": "c_tokenizer.py",
    "command_executable": "python3",
    "input_as_argument": false,
    "command_args": [
      "--ignore_comments"
    ],
    "token_value": "type"
  },
  "java": {
    "tokenizer": "java_tokenizer.py",
    "command_executable": "python3",
    "input_as_argument": true,
    "token_value": "type"
  },
  "mips": {
    "tokenizer": "mips_tokenizer.py",
